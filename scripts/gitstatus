for D1 in {psyche,highland2}
do
  cd $D1 
  for D2 in *
  do
  if [ ! -d $D2 ]; then
    continue
  fi
  cd $D2/v0r0
    echo 'folder '$D1'/'$D2'/v0r0'
    git status | grep "modified:"
    git status | grep "new:"
    git status | grep ahead
    git status | grep behind
    cd ../..
  done
  cd ..
done


